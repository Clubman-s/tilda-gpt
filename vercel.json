{
  "version": 2,
  "builds": [
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "methods": ["GET"]
    }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  }
}
